{
  "name": "vue3-parallax",
  "author": "Joaquin Abenza <jjabenza81@gmail.com> (https://github.com/joaquinabenza)",
  "version": "1.0.1",
  "license": "MIT",
  "description": "A set of vue3 parallax components",
  "private": false,
  "type": "module",
  "contributors": [
    {
      "name": "Joaquin Abenza",
      "email": "jjabenza81@gmail.com"
    }
  ],
  "keywords": [
    "vue3",
    "responsive",
    "dashboard",
    "components",
    "drag&drop",
    "dragable",
    "resizable",
    "grid",
    "layout",
    "widgets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Distribuciones-Notariales/vue3-parallax"
  },
  "homepage": "https://Distribuciones-Notariales.github.io/vue3-parallax/",
  "bugs": {
    "url": "hhttps://github.com/Distribuciones-Notariales/vue3-parallax/issues"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vue3-parallax.umd.cjs",
  "module": "./dist/vue3-parallax.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-parallax.js",
      "require": "./dist/vue3-parallax.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite --port 5180",
    "build": "npm run test && vite build --mode production && npm run generate:types && npm run build:pages",
    "build:lib": "npm run test && vite build --mode production && npm run generate:types",
    "clean": "rm -rf ./dist && rm -rf ./live-demo && rm -rf ./coverage",
    "coverage:web": "http-server ./coverage",
    "generate:types": "vue-tsc -p tsconfig-build.json --declaration --emitDeclarationOnly true --outdir ./dist",
    "changeset": "changeset",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run  --coverage",
    "build:pages": "npm run docs:build && npm run build:live-demo",
    "build:live-demo": "vite build --mode live-demo --outDir ./docs/.vitepress/dist/live-demo",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "ste-simple-events": "^3.0.11",
    "vue": "^3.3.8",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/node": "^20.9.4",
    "@vitest/coverage-v8": "^0.34.6",
    "typescript": "^5.6.2",
    "vite": "^5.0.0",
    "vitepress": "^1.0.0-rc.24",
    "vitest": "^0.34.6",
    "vue-tsc": "^2.0.29"
  }
}
